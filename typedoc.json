{
  "$schema": "https://typedoc.org/schema.json",
  
  "entryPoints": ["./src/index.ts"],
  "entryPointStrategy": "expand",
  
  "out": "./docs-site/docs/api",
  
  "plugin": ["typedoc-plugin-markdown"],
  
  "readme": "none",
  "name": "cifer-sdk API Reference",
  
  "includeVersion": true,
  "disableSources": false,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": false,
  "excludeExternals": true,
  
  "categorizeByGroup": true,
  "categoryOrder": [
    "SDK Factory",
    "Adapters",
    "Key Management",
    "Blackbox",
    "Commitments",
    "Flows",
    "Types",
    "Errors",
    "Internal",
    "*"
  ],
  
  "navigation": {
    "includeCategories": true,
    "includeGroups": true,
    "includeFolders": true
  },
  
  "sort": ["source-order", "alphabetical"],
  "kindSortOrder": [
    "Function",
    "Class",
    "Interface",
    "TypeAlias",
    "Variable",
    "Enum"
  ],
  
  "validation": {
    "notExported": false,
    "invalidLink": true,
    "notDocumented": false
  },
  
  "treatWarningsAsErrors": false,
  "treatValidationWarningsAsErrors": false,
  
  "tsconfig": "./tsconfig.json",
  
  "commentStyle": "jsdoc",
  
  "blockTags": [
    "@param",
    "@returns",
    "@throws",
    "@example",
    "@remarks",
    "@see",
    "@since",
    "@deprecated",
    "@default",
    "@defaultValue",
    "@typeParam",
    "@module",
    "@description"
  ],
  
  "inlineTags": [
    "@link",
    "@inheritDoc",
    "@label"
  ],
  
  "modifierTags": [
    "@public",
    "@protected",
    "@private",
    "@internal",
    "@readonly",
    "@packageDocumentation",
    "@eventProperty",
    "@alpha",
    "@beta",
    "@experimental",
    "@sealed",
    "@virtual",
    "@override"
  ],
  
  "hideGenerator": true,
  "searchInComments": true,
  
  "outputFileStrategy": "modules",
  "flattenOutputFiles": false,
  "fileExtension": ".md",
  
  "entryFileName": "index",
  "modulesFileName": "index",
  "membersWithOwnFile": ["Class", "Interface", "TypeAlias", "Function"]
}
